generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product{
  id Int @id @default(autoincrement())
  title String @unique
  price Decimal
  description String?
  category String
  image String?
  // createdAt DateTime @default(now())
  // updatedAt DateTime @updatedAt
}